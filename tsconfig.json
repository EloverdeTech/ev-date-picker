{
	"compileOnSave": false,
	"compilerOptions": {
		"baseUrl": "./",
		"outDir": "./dist/out-tsc",
		"sourceMap": true,
		"declaration": false,
		"module": "esnext",
		"moduleResolution": "node",
		"experimentalDecorators": true,
		"importHelpers": true,
		"target": "ES2022",
		"types": [
			"node"
		],
		"typeRoots": [
			"node_modules/@types"
		],
		"lib": [
			"es2018",
			"dom"
		],
		"paths": {
			"ev-date-picker": [
				"dist/ev-date-picker"
			],
			"ev-date-picker/*": [
				"dist/ev-date-picker/*"
			]
		},
		"useDefineForClassFields": false
	}
}